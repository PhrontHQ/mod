{
    "root": {
        "prototype": "core/meta/module-object-descriptor",
        "properties": {
            "name": "Polygon",
            "parent": {"@": "geometry"},
            "propertyDescriptors": [
                {"@": "identifier"},
                {"@": "coordinates"}
            ],
            "exportName": "Polygon",
            "module": {
                "%": "./polygon"
            }
        }
    },

    "geometry": {
        "object": "./geometry.mjson"
    },

    "identifier": {
        "prototype": "core/meta/property-descriptor",
        "properties": {
            "name": "identifier",
            "valueType": "string"
        }
    },

    "PositionDescriptor": {
        "object": "./position.mjson"
    },

    "coordinates": {
        "prototype": "core/meta/property-descriptor",
        "properties": {
            "name": "coordinates",
            "cardinality": -1,
            "valueType": "array",
            "valueDescriptor": {"@": "PositionDescriptor"}
        }
    }

}
